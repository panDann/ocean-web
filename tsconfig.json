{
    "compilerOptions": {
      "allowSyntheticDefaultImports": true,
      "experimentalDecorators":true,
      "esModuleInterop": true,
      "jsx": "react",
      "resolveJsonModule":true,
      "module": "commonjs",
      "noImplicitAny": true,
      "outDir": ".",
      "preserveConstEnums": true,
      "removeComments": true,
      "sourceMap": true,
      "target": "es5",
      "baseUrl": ".",
      "paths":{
          "@src/*":[
            "./src/*"
          ]
      }
    },
    "include": [
      "./src/**/*"
    ],
    "exclude":[
      "./mode_module/**/*"
    ]
    
  }