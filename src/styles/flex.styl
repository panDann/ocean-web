.flex-row {
    display flex
    flex-flow: row nowrap
    align-items: center
    }
.flex-column {
    display flex
    flex-flow: column nowrap
    align-items: center
    }
.justify-between {
    justify-content: space-between
    }
.justify-start {
    justify-content: flex-start
    }
.justify-around {
    justify-content: space-around
    }
.justify-center {
    justify-content: center
    }
.align-center {
    align-items: center
    }
